<h1>locations</h1>

<table>

<% @locations.each do |location| %>
  <tr>
    <td><%= link_to location.name, location %></td>
    <td><%=h location.address %></td>
    <td><%=h location.city %></td>
    <td><%=h location.state %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'add location', new_location_path %>